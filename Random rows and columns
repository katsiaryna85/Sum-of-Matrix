	//0,0 0,1 0,2, 0,3 0,4
		for(int i=0;i<N;i++)
		   System.out.print(A[0][i]+" ");
		// 1,N-1 2,N-1,...,N-1, N-1
		System.out.print('*');
		for (int i=1; i<N;i++ )
		   System.out.print( A[i][N-1]+" ");   
		System.out.print('*');   
		//N-1,N-2 N-1,N-3,... N-1,0   3 2 1 0
		for (int i=N-2; i>=0; i--)
		   System.out.print( A[N-1][i]+" ") ;
		System.out.print('*'); 
		// N-2,0 N-3,0...1,0
		for ( int i=N-2; i>=1; i--)
		    System.out.print( A[i][0]+" ");  
